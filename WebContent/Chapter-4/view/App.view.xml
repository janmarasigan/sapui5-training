<mvc:View
    controllerName="sapui5.training.template.controller.App"
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
    <App>
        <pages>
            <Page>
            	<content>
            		<Button text="Press me" press="onButtonPress" />
            		<List items="{items>/items}">
            			<StandardListItem title="{items>name}" description="{items>desc}" />
            		</List>
            	</content>
            </Page>
        </pages>
    </App>
</mvc:View>
