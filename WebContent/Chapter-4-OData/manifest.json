{
	"_version" : "1.8.0",
	"sap.app" : {
		"id" : "sapui5.training.template",
		"type" : "application",
		"i18n" : "i18n/i18n.properties",
		"title" : "{{appTitle}}",
		"description" : "{{appDescription}}",
		"applicationVersion" : {
			"vevrsion" : "1.0.0"
		},
		"dataSources" : {
			"items" : {
				"uri" : "model/Items.json",
				"type" : "JSON"
			},
			"northwind" : {
				"uri" : "http://services.odata.org/V3/Northwind/Northwind.svc/",
				"type" : "OData",
				"settings" : {
					"odataVersion" : "2.0"	
				}		
			}
		}
	},
	"sap.ui" : {
		"technology" : "UI5",
		"deviceTypes" : {
			"desktop" : true,
			"tablet" : true,
			"phone" : true
		}
	},
	"sap.ui5" : {
		"rootView" : {
			"viewName" : "sapui5.training.template.view.App",
			"type" : "XML",
			"id" : "app"
		},
		"dependencies" : {
			"minUI5Version" : "1.30",
			"libs" : {
				"sap.m" : {}
			}
		},
		"models" : {
			"i18n" : {
				"type" : "sap.ui.model.resource.ResourceModel",
				"settings" : {
					"bundleName" : "sapui5.training.template.i18n.i18n"
				}
			},
			"items" : {
				"type" : "sap.ui.model.json.JSONModel",
				"dataSource" : "items"
			},
			"" : {
				"dataSource" : "northwind"
			}
		}
	}
}
